{
  "name": "shop-me-boot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "i18next": "^10.5.0",
    "i18next-browser-languagedetector": "^2.1.0",
    "i18next-xhr-backend": "^1.5.1",
    "jest": "^20.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "client": "react-scripts start",
    "client:dev": "cross-env-shell REACT_APP_API=\"/mock-api\" npm run client",
    "client:prod": "cross-env-shell REACT_APP_API=\"/prod-api\" npm run client",
    "server": "pokemock swagger/swagger.json -p 3001",
    "test": "react-scripts test --env=jsdom",
    "build": "cross-env-shell REACT_APP_API=\"/prod-api\" react-scripts build",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest-fetch-mock": "^1.4.2",
    "pokemock": "^1.2.0"
  },
  "proxy": {
    "/prod-api": {
      "target": "https://patronage2018.intive-projects.com",
      "changeOrigin": true,
      "pathRewrite": {
        "^/prod-api": "/api"
      }
    },
    "/mock-api": {
      "target": "http://localhost:3001/",
      "changeOrigin": true,
      "pathRewrite": {
        "^/mock-api": "/"
      }
    }
  }
}
